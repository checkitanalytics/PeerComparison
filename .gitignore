cat > .gitignore << 'EOF'
# Environment & Secrets - NEVER COMMIT THESE!
.env
*.env
.env.local
.env.production

# Python
__pycache__/
*.py[cod]
*$py.class
.venv/
venv/
*.egg-info/
dist/
build/

# Testing - Keep test files, ignore test results
.pytest_cache/
test_report.html
htmlcov/
.coverage
local_test_log.txt
*_local_test.py

# IDE
.vscode/
.idea/
*.swp
*.swo

# OS
.DS_Store
Thumbs.db

# Logs
*.log

# Replit specific
.replit
.config/
.upm/
.cache/
.local/

# Local test data (NOT the test file itself)
local_*.json

# Temporary files
*.tmp
temp/
tmp/
EOF